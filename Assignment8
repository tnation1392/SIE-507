import numpy as np
import matplotlib.pyplot as plt

# Todd Nason SIE 507 Assignment 8

#1:
print('\n', "QUESTION 1")
randline = np.random.randint(11, 30, size=[6])  #Creates a 6-character array with random numbers between 11-29
print(randline)

#2:
print('\n', "QUESTION 2")
rand_three = np.random.random(size=[3, 3, 3])  #Creates a 3x3x3 array with random values
print(rand_three)

#3:
print('\n', "QUESTION 3")
rand_five = np.random.random(size=[5, 5])  #Creates a 5x5 array of random values
rand_five = np.array(rand_five)
print(rand_five)
print('\n', "Minimum value:", rand_five.min())  #Displays the minimum value in the array
print("", "Maximum value:", rand_five.max())  #Displays the maximum value in the array

#4:
print('\n', "QUESTION 4")
rand_orig = np.random.random(size=10)  #Creates a 10 character array
rand_orig = np.array(rand_orig)
rand_sort = np.sort(rand_orig)  #Sorts the array from least to greatest value
print("Original: ", rand_orig)
print("Sorted: ", rand_sort)

#5:
print('\n', "QUESTION 5")
file_data = np.genfromtxt("aircrafts.csv", delimiter=",", skip_header=1)
print(file_data)
